<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- This version uses the openai_ros environments -->
    <rosparam command="load" file="$(find mycobot_training)/config/mycobot_world_params.yaml" />
    <!-- Launch the training system -->
    <node pkg="mycobot_training" name="mycobot" type="start_training.py" output="screen"/>
</launch>
